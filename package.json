{
  "name": "@byted/light-render",
  "version": "0.1.0",
  "description": "a fast lightweight 2d graphic library",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "start": "npm-run-all -s build:worker dev",
    "build:worker": "webpack --config ./build/webpack.config.worker.js && node build/build-worker.js",
    "build": "npm-run-all -s build:worker build:es uglify",
    "dev": "node build/dev-server.js",
    "build:es": "./node_modules/.bin/tsc -d",
    "build:umd": "node build/build.js",
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register test/test.js",
    "tslint": "tslint -c tslint.json -r src/ -p ./tsconfig.json",
    "lint": "eslint --fix src/",
    "prettier": "prettier --write src/",
    "uglify": "node ./build/uglify.js"
  },
  "sideEffects": [
    "lib/render.js"
  ],
  "devDependencies": {
    "@types/color": "^3.0.1",
    "@types/hammerjs": "^2.0.45",
    "@types/lodash-es": "^4.17.7",
    "@types/throttle-debounce": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "babel-core": "^6.24.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.3.2",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "chai": "^4.1.2",
    "cross-env": "^5.2.1",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.21.1",
    "eslint-plugin-jest": "^24.0.1",
    "eslint-plugin-unicorn": "^23.0.0",
    "eslint-tinker": "^0.5.0",
    "hammerjs": "^2.0.8",
    "html-webpack-plugin": "^2.30.1",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "opn": "^5.1.0",
    "ora": "^2.1.0",
    "prettier": "^2.2.1",
    "ts-loader": "^3.2.0",
    "typescript": "^4.1.6",
    "uglify-js": "^3.17.4",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-dev-server": "^3.1.11",
    "webpack-hot-middleware": "2.13.2",
    "webpack-merge": "^4.1.0"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "liwensheng",
  "license": "ISC",
  "homepage": "https://github.com/oceanengine/okeevis-render",
  "dependencies": {
    "@egjs/list-differ": "1.0.0",
    "color": "3.1.3",
    "gl-matrix": "3.3.0",
    "lodash-es": "^4.17.21",
    "normalize-wheel": "1.0.1",
    "roughjs": "^4.6.6",
    "throttle-debounce": "^3.0.1"
  }
}
